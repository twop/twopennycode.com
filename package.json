{
  "name": "twopennycode.com",
  "description": "My personal blog built with gatsbyjs.",
  "version": "1.0.0",
  "private": true,
  "author": "Simon Korzunov",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "now-build": "npm run build",
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.4",
    "@emotion/styled": "^10.0.4",
    "babel-plugin-styled-components": "^1.9.2",
    "classnames": "^2.2.6",
    "emotion": "^10.0.4",
    "emotion-server": "^10.0.4",
    "gatsby": "^2.0.90",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-canonical-urls": "^2.0.8",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-prismjs": "^3.2.0",
    "gatsby-remark-responsive-iframe": "^2.0.7",
    "gatsby-remark-smartypants": "^2.0.7",
    "gatsby-source-filesystem": "^2.0.13",
    "gatsby-transformer-json": "^2.1.7",
    "gatsby-transformer-remark": "^2.2.0",
    "gatsby-transformer-sharp": "^2.1.10",
    "modern-normalize": "^0.5.0",
    "normalize.css": "^8.0.1",
    "polished": "^2.3.1",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.10.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "gh-pages": "^2.0.1",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1"
  }
}
